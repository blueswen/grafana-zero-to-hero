          "direction": "backward",
          "editorMode": "code",
          "expr": "avg_over_time({app_name=~\"$app_name\"} |= `$session_id` |= \"ttfb\" | logfmt | kind=\"measurement\" | unwrap ttfb [$__range]) by (app_name)",
